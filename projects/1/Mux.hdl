/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in= sel, out= notout);
    And(a= a, b= notout, out= and1out);
    And(a= b, b= sel, out= and2out);
    Or(a= and1out, b= and2out, out= out);
}